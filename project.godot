config_version=5

[application]

config/name="Space Cats"
config/version="1.0"
run/main_scene="res://source/scene/loading.tscn"
config/features=PackedStringArray("4.2")
run/max_fps=240
config/icon="res://icon.svg"

[autoload]

__InkRuntime="*res://addons/inkgd/ink_runtime.gd"
GlobalManager="*res://source/global/globalManager.gd"
ResourceManager="*res://source/global/resourceManager.gd"
SaveManager="*res://source/global/saveManager.gd"
SettingsManager="*res://source/global/settingsManager.gd"
SceneManager="*res://source/global/sceneManager.gd"
DialogueManager="*res://source/global/dialogueManager.gd"
InputManager="*res://source/global/inputManager.gd"
SoundManager="*res://source/global/soundManager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=2
mouse_cursor/tooltip_position_offset=Vector2(30, 24)

[editor_plugins]

enabled=PackedStringArray("res://addons/aseprite_importer/plugin.cfg", "res://addons/inkgd/plugin.cfg")

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom="res://assets/theme.tres"
timers/tooltip_delay_sec=0.0

[inkgd]

register_templates=true

